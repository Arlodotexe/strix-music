<ResourceDictionary
	x:Class="StrixMusic.Sdk.WinUI.Styles.Views.SearchViewStyle"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:converters="using:StrixMusic.Sdk.WinUI.Converters"
	xmlns:sdk="using:StrixMusic.Sdk"
	xmlns:views="using:StrixMusic.Sdk.WinUI.Controls.Views">

	<Style x:Key="DefaultSearchViewStyle" TargetType="views:SearchView">
		<Style.Setters>
			<Setter Property="Template">
				<Setter.Value>
					<ControlTemplate TargetType="views:SearchView">
						<StackPanel Orientation="Horizontal">
							<TextBlock Text="Search results for &quot;" />
							<TextBlock Text="Not implemented" />
							<TextBlock Text="&quot;" />
						</StackPanel>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style.Setters>
	</Style>

	<Style BasedOn="{StaticResource DefaultSearchViewStyle}" TargetType="views:SearchView" />

</ResourceDictionary>