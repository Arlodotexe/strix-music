<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:views="using:StrixMusic.Sdk.Uno.Controls.Views"
    xmlns:converters="using:StrixMusic.Sdk.Uno.Converters"
    xmlns:sdk="using:StrixMusic.Sdk"
    x:Class="StrixMusic.Sdk.Uno.Styles.Views.SearchViewStyle">

    <DataTemplate x:Key="DefaultSearchViewTemplate" x:DataType="sdk:MainViewModel">
       
    </DataTemplate>

    <Style x:Key="DefaultSearchViewStyle" TargetType="views:SearchView">
        <Style.Setters>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="views:SearchView">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="Search results for &quot;"/>
                            <TextBlock Text="Not implemented"/>
                            <TextBlock Text="&quot;"/>
                        </StackPanel>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <Style BasedOn="{StaticResource DefaultSearchViewStyle}" TargetType="views:SearchView"/>

</ResourceDictionary>