<UserControl
    x:Class="StrixMusic.Shell.ZuneDesktop.Controls.Overlay"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="using:StrixMusic.Shell.ZuneDesktop.Controls"
    xmlns:sys="using:System"
    x:Name="RootControl"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ZuneDarkBackground}">
        <Button Content="Leave CompactOverlay" Click="Button_Click" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Transparent"/>
    </Grid>
</UserControl>
