<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:StrixMusic.Shells.ZuneDesktop.Controls"
    xmlns:defaultcontrols="using:StrixMusic.Sdk.Uno.Controls"
    x:Class="StrixMusic.Shells.ZuneDesktop.Styles.Sdk.SettingsViewStyle">
    
    <Style x:Key="ZuneDesktopSettingsViewStyle" TargetType="defaultcontrols:SettingsView">
        <Style.Setters>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="defaultcontrols:SettingsView">
                        <local:SettingsView/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <Style BasedOn="{StaticResource ZuneDesktopSettingsViewStyle}" TargetType="defaultcontrols:SettingsView"/>

</ResourceDictionary>
