<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:views="using:StrixMusic.Sdk.Uno.Controls.Views"
    xmlns:settingsviews="using:StrixMusic.Shells.ZuneDesktop.Controls.Views.Settings"
    x:Class="StrixMusic.Shells.ZuneDesktop.Styles.Views.SettingsViewStyle">

    <Style x:Key="ZuneDesktopSettingsViewStyle" TargetType="views:SettingsView">
        <Style.Setters>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="views:SettingsView">
                        <settingsviews:SettingsView/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>

    <Style BasedOn="{StaticResource ZuneDesktopSettingsViewStyle}" TargetType="views:SettingsView"/>

</ResourceDictionary>
