<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Name="Dark">
            <SolidColorBrush x:Key="LowBackgroundBrush" Color="#FF333333"/>
        </ResourceDictionary>
        <ResourceDictionary x:Name="Light">
            <SolidColorBrush x:Key="LowBackgroundBrush" Color="#FFCCCCCC"/>
        </ResourceDictionary>
        <ResourceDictionary x:Name="Default">
            <SolidColorBrush x:Key="LowBackgroundBrush" Color="#FF333333"/>
        </ResourceDictionary>
        <ResourceDictionary x:Name="HighContrast">
            <SolidColorBrush x:Key="LowBackgroundBrush" Color="#FF333333"/>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

</ResourceDictionary>
