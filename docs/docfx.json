{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Sdk/StrixMusic.Sdk/StrixMusic.Sdk.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "/wwwroot/**"
          ],
          "src": "../"
        }
      ],
      "dest": "reference/api"
    }
  ],
  "build": {
    "resource": {
      "files": [
        "assets/**"
      ]
    },
    "content": [
      {
        "files": [
          "reference/**.yml",
          "reference/**.md"
        ]
      },
      {
        "files": [
          "index.md",
          "toc.yml",
          "**.md"
        ]
      },
      {
        "files": [
          "cores/**.md",
          "cores/**.yml"
        ]
      },
      {
        "files": [
          "shells/**.md",
          "shells/**.yml"
        ]
      },
      {
        "files": [
          "get-started/**.md",
          "get-started/**.yml"
        ]
      }
    ],
    "dest": "wwwroot",
    "template": [
      "default",
      "templates/strix"
    ],
    "globalMetadata": {
      "_appTitle": "Strix Music",
      "_appFooter": "Â© 2022 Arlo Godfrey",
      "_appLogoPath": "assets/logos/AppIcon.svg",
      "_enableSearch": true,
      "_appFaviconPath": "assets/favicon.ico"
    },
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}