{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Sdk/StrixMusic.Sdk/StrixMusic.Sdk.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "/wwwroot/**"
          ],
          "src": "../"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "index.md",
          "toc.yml",
          "**.md"
        ]
      }
    ],
    "dest": "wwwroot",
    "template": [
      "default"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}